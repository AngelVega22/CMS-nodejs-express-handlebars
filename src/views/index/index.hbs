{{#if user.is_superuser}}
<div class="bg-admin">
  <h1 class="text-center text-white position-absolute top-50 start-50 translate-middle">Bienvenido
    {{user.first_name}}{{user.last_name}}
  </h1>
</div>
{{else}}


{{>inicio/banner}}
<main class="container">



  <div>


    <section class="featured spad">
      <div class="container">
        <div class="row">
          <div class="section-title mb-5">
            <h3 class="text-center mt-5">Categoría de cursos
              {{!-- <i class="fas fa-plus-circle fa-spin"></i> --}}
            </h3>
            <div class="d-flex justify-content-center mt-3">
              <p class="text-center w-50 mb-3">
                Contamos con profesionales especialistas en Marketing tradicional y Digital, Investigaciones de Mercado,
                Diseño y programación web, entre otros.
              </p>
            </div>

          </div>
          <div class="col-lg-12">
            <div class="section-title">
            </div>

          </div>
        </div>


        <div class="row ">
          {{#each data}}
          {{#if foto}}
          {{#if estado_categoria}}
          <!--classp-1 ms-5 bg-cate-->
          <div class="col-md-6 col-lg-4 col-xl-3  mb-5 p-3   div-cat  ">
            <a class="text-dark text-decoration-none" href="/cursos-categoria/{{id}}">

              <img src="{{foto}}" class=" foto-categoria" alt="" srcset="">


              <h4 class=" text-center text-black titulo-categoria mb-4">{{nombre}} </h4>


            </a>
          </div>

          {{/if}}

          {{/if}}
          {{/each}}
        </div>

</main>
{{/if}}
